class Pen{
    String color;
    String type;
    public void Write(){
        System.out.println("Write something about yours");
       
    }
    public void printColor(){
            system.out.println(this.color);

    }
}
public class OOPS {
    public static void main(String args[]) {
Pen pen1 = new Pen();
pen1.color = "Yellow";
pen1.type = "Gel";
Pen pen2 = new Pen();
pen2.color = "white";
pen2.type = "marker";
pen1.PrintColor();
Pen2.printColor();


    }

}
